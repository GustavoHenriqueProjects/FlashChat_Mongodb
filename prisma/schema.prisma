// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum TypeUser {
  CLIENT
  DIARIST
}

model RegisterUser {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  typeUser TypeUser
  userMysqlId Int 
  name String
  photoUrl String
}

model Chat {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  serviceMysqlId Int
  senderId Int
  recipientId Int
  message String
  date DateTime
}